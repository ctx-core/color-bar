{
	"name": "@ctx-core/color-bar",
	"version": "14.0.77",
	"description": "ctx-core color-bar",
	"keywords": [
		"ctx-core",
		"color-bar"
	],
	"homepage": "https://github.com/ctx-core/color-bar#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/color-bar/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/color-bar.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.0.33",
		"@ctx-core/ctx-core-package-tools": "workspace:^8.0.86",
		"@ctx-core/html": "workspace:^11.0.29"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.120",
		"svelte": "^3.44.3",
		"svelte-preprocess": "^4.10.0",
		"svelte2tsx": "^0.4.11",
		"tslib": "^2.3.1",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
